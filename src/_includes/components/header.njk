<header class="c-header">
  <a class="c-header__branding" href="/">{% include "components/logo.njk" %}</a>
  <nav class="c-header__nav">
    <ul class="c-header__nav-list">
      {% for item in navigation.primary %}
      <li class="c-header__nav-item">
        <a class="c-header__nav-anchor" {% if page.url == item.url %}aria-current="page"{% endif %} href="{{ item.url }}">{{ item.label }}</a>
      </li>
      {% endfor %}
    </ul>
  </nav>
</header>
