{
  "name": "alexcarpenter-me",
  "version": "1.0.0",
  "author": "Alex Carpenter",
  "scripts": {
    "build:files": "eleventy --config config/eleventy.config.js --quiet",
    "build:scripts": "rollup --config config/rollup.config.js",
    "build:styles": "postcss src/_assets/styles/app.css -o www/assets/styles/app.css --config config/postcss.config.js",
    "copy:fonts": "cp -rf src/_assets/fonts www/assets/fonts",
    "clean": "rm -rf www",
    "build": "npm run build:scripts && npm run build:styles && npm run build:files && npm run copy:fonts",
    "watch": "npx eleventy --config config/eleventy.config.js --watch"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.3.3",
    "postcss-cli": "^5.0.0",
    "postcss-easy-import": "^3.0.0",
    "rollup": "^0.58.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "dependencies": {}
}
