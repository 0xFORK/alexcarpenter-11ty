{
  "name": "alexcarpenter-me",
  "author": "Alex Carpenter",
  "version": "1.0.0",
  "scripts": {
    "styles": "postcss src/stylesheets/main.css -o dist/assets/css/main.css",
    "scripts": "rollup -c config/rollup/config.js -w",
    "jekyll": "jekyll serve -c config/jekyll/config.yml",
    "deploy": "push-dir --dir=dist --branch=gh-pages"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "cssnano": "^3.10.0",
    "fontfaceobserver": "^2.0.13",
    "postcss": "^6.0.6",
    "postcss-cli": "^4.1.0",
    "postcss-import": "^10.0.0",
    "postcss-load-config": "^1.2.0",
    "push-dir": "^0.4.1",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^3.2.2"
  },
  "postcss": {
    "map": false,
    "plugins": {
      "postcss-import": {},
      "autoprefixer": {},
      "cssnano": {}
    }
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "external-helpers"
    ]
  }
}
